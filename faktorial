#include <iostream>
using namespace std;

int main() {
	int n;
	long double factorial = 1.0;

	cout << "Enter a positive integer ";
	cin >> n;

	if (n < 0)
		cout << "Error! Faktorialne cislo neexistuje.";
	else {
		for (int i = 1; i <= n; ++i) {
			factorial *= i;
		}
		cout << "faktorial z " << n << " = " << factorial;
	}
	return 0;
}
